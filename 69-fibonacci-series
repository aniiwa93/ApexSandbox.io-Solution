public List<Integer> fibonacciSeries(Integer n) {
    List<Integer> nowyFibonacci = new List<Integer>();
    if(n == 1){
        return 1;
    if(n == 2){
        nowyFibonacci.add(1);
        nowyFibonacci.add(1);
        return nowyFibonacci;
    }
    if (n > 2){
    for(Integer i = 0; i < n; i++){
        nowyFibonacci.add(fibonacciSeries[i]);
    }
    }
    return nowyFibonacci;
}