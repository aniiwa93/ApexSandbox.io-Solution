public List<Integer> fibonacciSeries(Integer n) {

    if(n == 1){
        return new List<Integer> {1};
    List<Integer> nowyFibonacci = new List<Integer> {1, 1};
    for(Integer i = 2; i < n; i++){
        nowyFibonacci.add(nowyFibonacci[i-1] + nowyFibonacci[i-2]);
    }
    return nowyFibonacci;
}