public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    List<String> fullNames = new List<String>();
    for(Integer i = 0, j = 0; i < firstNames.size(), j < lastNames.size(); i++,j++ ){
        fullNames.add(firstNames[i] + ' ' + lastNames[j]);
    }
return fullNames;
}