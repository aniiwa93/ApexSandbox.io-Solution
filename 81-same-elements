public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {
    Set<Integer> set1 = new Set<Integer>(nums1);
    Set<Integer> set2 = new Set<Integer>(nums2);

    for(Integer n : set1){
        if(set2.contains(n){
            return true;
        }
        else {
            return false;
        }
      }
    }